{
  "name": "@mui/studio-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn esinstall && concurrently \"yarn:dev:*\"",
    "build": "yarn esinstall && concurrently \"yarn:build:*\"",
    "start": "next start",
    "lint": "next lint && prettier --check .",
    "fix": "next lint --fix && prettier --write .",
    "build:next": "next build",
    "dev:next": "next dev",
    "build:sandbox": "tsc --build ./sandbox/main",
    "dev:sandbox": "yarn build:sandbox --pretty --preserveWatchOutput --watch",
    "esinstall": "node ./scripts/esinstall.mjs"
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.7.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.57",
    "@mui/material": "^5.2.1",
    "@mui/studio-core": "^0.0.1",
    "@mui/styles": "^5.2.1",
    "@mui/x-data-grid": "^5.0.0-beta.7",
    "@types/cors": "^2.8.12",
    "@types/prettier": "^2.4.2",
    "@types/react-dom": "^17.0.11",
    "clsx": "^1.1.1",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "es-module-shims": "^1.3.3",
    "esbuild": "^0.14.0",
    "esinstall": "^1.1.7",
    "next": "^12.0.4",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sucrase": "^3.20.3",
    "swr": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.34",
    "eslint": "7",
    "eslint-config-next": "^12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "typescript": "^4.4.3"
  },
  "typings": "./index.d.ts"
}
