{
  "name": "@mui/toolpad",
  "version": "0.1.34",
  "license": "MIT",
  "bin": {
    "toolpad": "./cli.js"
  },
  "scripts": {
    "build": "concurrently \"yarn:build:*\"",
    "dev": "concurrently \"yarn:dev:*\"",
    "cli": "./cli.js",
    "lint": "prettier --check .",
    "fix": "prettier --write .",
    "build:cli": "tsup",
    "build:editor": "vite build ./src/toolpad",
    "build:typings": "tsx ./scripts/typings.mts",
    "dev:cli": "tsup --watch",
    "dev:typings": "yarn build:typings",
    "waitForApp": "tsx ./scripts/waitForApp.mts",
    "check-types": "tsup && tsc --noEmit",
    "test": "vitest run"
  },
  "files": [
    "public",
    "scripts",
    "dist",
    "cli.js",
    "browser",
    "server",
    "runtime"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./cli": {
      "require": "./dist/cli/index.js"
    },
    ".": {
      "types": "./dist/exports/index.d.ts",
      "import": "./dist/exports/index.mjs",
      "require": "./dist/exports/index.js"
    },
    "./*": {
      "types": "./dist/exports/*.d.ts",
      "import": "./dist/exports/*.mjs",
      "require": "./dist/exports/*.js"
    }
  },
