{
  "name": "@mui/toolpad-app",
  "version": "0.0.5-alpha.4",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma generate && yarn waitForDb && yarn prisma db push --skip-generate && yarn esinstall --dev && concurrently \"yarn:dev:*\"",
    "build": "yarn prisma generate && yarn esinstall && concurrently \"yarn:build:*\" && rimraf ./.next/cache",
    "start": "yarn waitForDb && yarn prisma migrate deploy && NODE_ENV=production node ./server.js",
    "lint": "next lint && prettier --check .",
    "fix": "next lint --fix && prettier --write .",
    "esinstall": "ts-node ./scripts/esinstall.ts",
    "esbuildEsm": "ts-node ./scripts/esbuildEsm.ts",
    "prisma": "prisma",
    "build:next": "next build",
    "build:esbuild": "rimraf ./public/sandbox ./public/editorRuntime && esbuild ./sandbox/index.ts ./editorRuntime/index.ts --target=es6 --bundle --outdir=./public",
    "build:types": "ts-node ./scripts/typings.ts",
    "build:prisma": "prisma generate",
    "dev:next": "node ./server.js",
    "dev:esbuild": "yarn build:esbuild --watch",
    "dev:types": "yarn build:types",
    "dev:prisma": "prisma generate --watch",
    "waitForDb": "ts-node ./scripts/waitForDb.ts"
  },
  "files": [
    "prisma",
    "public",
    "scripts",
    ".next"
  ],
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.2",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@monaco-editor/react": "^4.4.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.75",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "@mui/toolpad-components": "^0.0.5-alpha.4",
    "@mui/toolpad-core": "^0.0.5-alpha.4",
    "@mui/x-data-grid-pro": "^5.7.0",
    "@prisma/client": "^3.11.1",
    "@swc/wasm": "^1.2.163",
    "@types/cors": "^2.8.12",
    "@types/prettier": "^2.4.3",
    "@types/react-dom": "^17.0.14",
    "arg": "^5.0.1",
    "basic-auth": "^2.0.1",
    "clsx": "^1.1.1",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csstype": "^3.0.11",
    "cuid": "^2.1.8",
    "es-module-shims": "^1.5.4",
    "esbuild": "^0.14.31",
    "esinstall": "^1.1.7",
    "find-up": "^6.2.0",
    "fractional-indexing": "^2.0.0",
    "googleapis": "^97.0.0",
    "import-meta-resolve": "^1.1.1",
    "json-to-ts": "^1.7.0",
    "monaco-editor": "^0.33.0",
    "next": "^12.1.4",
    "path-to-regexp": "^6.2.0",
    "prettier": "^2.6.2",
    "quickjs-emscripten": "^0.15.0",
    "react": "^18.0.0",
    "react-devtools-inline": "^4.24.3",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-json-view": "^1.21.3",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.3.0",
    "react-split-pane": "^0.1.92",
    "sucrase": "^3.20.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.43",
    "ajv": "^8.11.0",
    "eslint": "7",
    "eslint-config-next": "^12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "glob": "^7.2.0",
    "prisma": "^3.11.1"
  },
  "typings": "./index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
