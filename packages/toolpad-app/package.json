    "dev": "concurrently \"yarn:dev:*\"",
    "cli": "./cli.js",
    "lint": "next lint && prettier --check .",
    "fix": "next lint --fix && prettier --write .",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:next": "NODE_OPTIONS=--max-old-space-size=8192 next build",
