    "dev:cli": "yarn build:cli --watch",
    "dev:react-devtools": "yarn build:react-devtools --watch",
    "dev:typings": "yarn build:typings",
    "waitForApp": "ts-node --esm ./scripts/waitForApp.mts",
    "check-types": "tsc --noEmit"
  },
