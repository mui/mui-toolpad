{
  "name": "@mui/toolpad-components",
  "version": "0.1.49",
  "description": "Build MUI apps quickly",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/mui-toolpad#readme",
  "license": "MIT",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./Button": {
      "types": "./dist/Button.d.ts",
      "default": "./dist/Button.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-toolpad.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsup && tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/mui/mui-toolpad/issues"
  },
  "dependencies": {
    "@mui/icons-material": "5.15.10",
    "@mui/lab": "5.0.0-alpha.165",
    "@mui/material": "5.15.10",
    "@mui/toolpad-core": "workspace:*",
    "@mui/toolpad-utils": "workspace:*",
    "@mui/x-charts": "6.19.4",
    "@mui/x-data-grid-pro": "6.19.4",
    "@mui/x-date-pickers": "6.19.4",
    "@mui/x-license-pro": "6.10.2",
    "@tanstack/react-query": "5.18.1",
    "dayjs": "1.11.10",
    "invariant": "2.2.4",
    "markdown-to-jsx": "7.4.1",
    "react-error-boundary": "4.0.12",
    "react-hook-form": "7.50.0"
  },
  "devDependencies": {
    "@types/invariant": "2.2.37",
    "@types/react": "18.2.55",
    "react": "18.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
