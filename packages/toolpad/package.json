{
  "name": "@mui/toolpad",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "cjsify": "mkdir -p ./dist/cjs && echo \"{ \\\"type\\\":\\\"commonjs\\\" }\" > ./dist/cjs/package.json",
    "build": "concurrently \"yarn:build:*\" && yarn cjsify",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev": "concurrently \"yarn:dev:*\" yarn:cjsify",
    "dev:esm": "yarn build:esm --watch",
    "dev:cjs": "yarn build:cjs --watch"
  },
  "exports": {
    "./package.json": "./package.json",
    "./server": {
      "types": "./dist/esm/server.d.ts",
      "import": "./dist/esm/server.js",
      "require": "./dist/cjs/server.js"
    },
    "./browser": {
      "types": "./dist/esm/browser.d.ts",
      "import": "./dist/esm/browser.js",
      "require": "./dist/cjs/browser.js"
    }
  },
  "files": [
    "index.cjs",
    "dist",
    "server",
    "browser"
  ],
  "bin": {
    "toolpad": "./index.cjs"
  },
  "dependencies": {
    "@mui/toolpad-app": "^0.1.2",
    "@mui/toolpad-core": "^0.1.2",
    "arg": "^5.0.2",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "execa": "^7.1.1"
  },
  "engines": {
    "node": ">=16.17"
  },
  "gitHead": "c21fac688cfd8270cf36126d215698ae989ff899"
}
