{
  "name": "@mui/toolpad-core",
  "version": "0.0.42-alpha.6",
  "description": "Build MUI apps quickly",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/mui-toolpad#readme",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-toolpad.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "dev13": "yarn dev",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "check-types": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/mui/mui-toolpad/issues"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.27.0",
    "mitt": "^3.0.0",
    "quickjs-emscripten": "^0.22.0",
    "react-error-boundary": "^3.1.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "035964a9ef748b365e130b428c2cbba9663902cf"
}
