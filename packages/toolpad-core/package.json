{
  "name": "@mui/toolpad-core",
  "version": "0.0.42-alpha.2",
  "description": "Build MUI apps quickly",
  "author": "MUI Toolpad team",
  "homepage": "https://github.com/mui/mui-toolpad#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-toolpad.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "dev13": "yarn dev",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "check-types": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/mui/mui-toolpad/issues"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.24.10",
    "mitt": "^3.0.0",
    "quickjs-emscripten": "^0.22.0",
    "react-error-boundary": "^3.1.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "10b25c037c933afa70a7a01a173249babfd43409"
}
