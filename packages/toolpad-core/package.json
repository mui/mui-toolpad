{
  "name": "@toolpad/core",
  "version": "0.0.1",
  "keywords": [
    "toolpad"
  ],
  "description": "Build dashboards fast.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mui/mui-toolpad.git",
    "directory": "packages/toolpad-core"
  },
  "bugs": {
    "url": "https://github.com/mui/mui-toolpad/issues"
  },
  "author": "Toolpad Team",
  "homepage": "https://github.com/mui/mui-toolpad#readme",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsup && tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
