{
  "name": "@mui/studio-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn prisma db push && yarn esinstall --dev && concurrently \"yarn:dev:*\"",
    "build": "yarn prisma generate && yarn esinstall && concurrently \"yarn:build:*\"",
    "start": "next start",
    "lint": "next lint && prettier --check .",
    "fix": "next lint --fix && prettier --write .",
    "esinstall": "ts-node ./scripts/esinstall.ts",
    "esbuildEsm": "ts-node ./scripts/esbuildEsm.ts",
    "prisma": "prisma",
    "build:next": "next build",
    "build:esbuild": "esbuild ./sandbox/index.ts ./editorRuntime/index.ts --target=es6 --bundle --outdir=./public",
    "build:types": "ts-node ./scripts/typings.ts",
    "build:prisma": "prisma generate",
    "dev:next": "next dev",
    "dev:esbuild": "yarn build:esbuild --watch",
    "dev:types": "yarn build:types",
    "dev:prisma": "prisma generate --watch"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/studio-components": "0.0.1",
    "@mui/studio-core": "^0.0.1",
    "@mui/styles": "^5.3.0",
    "@mui/x-data-grid-pro": "^5.4.0",
    "@prisma/client": "^3.9.2",
    "@types/cors": "^2.8.12",
    "@types/prettier": "^2.4.3",
    "@types/react-dom": "^17.0.11",
    "arg": "^5.0.1",
    "clsx": "^1.1.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "es-module-shims": "^1.4.3",
    "esbuild": "^0.14.14",
    "esinstall": "^1.1.7",
    "find-up": "^6.2.0",
    "fractional-indexing": "^2.0.0",
    "import-meta-resolve": "^1.1.1",
    "json-to-ts": "^1.7.0",
    "monaco-editor": "^0.32.1",
    "next": "^12.0.8",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-devtools-inline": "^4.23.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-json-view": "^1.21.3",
    "react-query": "^3.34.12",
    "react-router-dom": "^6.2.1",
    "sucrase": "^3.20.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.34",
    "ajv": "^8.9.0",
    "eslint": "7",
    "eslint-config-next": "^12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "glob": "^7.2.0",
    "prisma": "^3.9.2"
  },
  "typings": "./index.d.ts"
}
