    "monaco-editor": "^0.31.1",
    "next": "^12.0.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-devtools-inline": "^4.22.1",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.7",
    "react-router-dom": "^6.2.1",
    "sqlite3": "^5.0.2",
    "sucrase": "^3.20.3",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.34",
    "eslint": "7",
    "eslint-config-next": "^12.0.3",
