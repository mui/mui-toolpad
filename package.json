{
  "name": "mui-studio",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Build MUI apps quickly",
  "repository": "https://github.com/mui-org/mui-studio.git",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "docs"
    ]
  },
  "bin": {
    "studio": "./dist/index.js"
  },
  "scripts": {
    "prettier": "node scripts/prettier.js",
    "deduplicate": "node scripts/deduplicate.js",
    "start": "cross-env FORCE_COLOR=1 lerna run --stream --scope @mui/studio start",
    "dev": "cross-env FORCE_COLOR=1 lerna run dev --stream --parallel --scope @mui/studio-core --scope @mui/studio-components --scope @mui/studio",
    "cli": "cross-env FORCE_COLOR=1 packages/studio-cli/cli.js --dev",
    "eslint": "eslint . --report-unused-disable-directives --ext .js,.ts,.tsx",
    "prima": "yarn workspace @mui/studio-app run --cwd=\"./packages/studio-app\"  prisma",
    "release:version": "lerna version --no-changelog --no-push --no-git-tag-version",
    "release:build": "lerna run --stream build",
    "release:publish": "lerna publish from-package --dist-tag alpha",
    "release:publish:dry-run": "yarn release:publish --registry=\"http://localhost:4873/\""
  },
  "devDependencies": {
    "@mui/monorepo": "git+https://github.com/mui-org/material-ui.git#master",
    "@swc/core": "^1.2.133",
    "@swc/helpers": "^0.3.2",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "format-util": "^1.0.5",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "^3.9.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "eslint-plugin-react": "7.27.1"
  }
}
